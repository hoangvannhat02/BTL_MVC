<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js" lang="en">
<!--<![endif]-->

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="apple-icon.png">
    <link rel="shortcut icon" href="~/Areas/Admin/Data/favicon.ico">



    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/themify-icons/css/themify-icons.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/selectFX/css/cs-skin-elastic.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/jqvmap/dist/jqvmap.min.css">

    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/datatables.net-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Data/vendors/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css">


    <link rel="stylesheet" href="~/Areas/Admin/Data/assets/css/style.css">

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>

    @*<link data-require="bootstrap-css" data-semver="3.3.1" rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" />*@

    <link href="~/pagibstrap.css" rel="stylesheet" />
    

</head>

<body>


    <!-- Left Panel -->

    <aside id="left-panel" class="left-panel">
        <nav class="navbar navbar-expand-sm navbar-default">

            <div class="navbar-header">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-menu" aria-controls="main-menu" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand" href="./"><img src="~/Areas/Admin/Data/images/logo.png" alt="Logo"></a>
                <a class="navbar-brand hidden" href="./"><img src="~/Areas/Admin/Data/images/logo2.png" alt="Logo"></a>
            </div>

            <div id="main-menu" class="main-menu collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active">
                        <a href="/Admin/Admin/Index"> <i class="menu-icon fa fa-dashboard"></i>Tổng quan </a>
                    </li>
                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/ProductsCategories/ViewData"> <i class="menu-icon fa fa-laptop"></i>Quản lý loại sản phẩm</a>

                    </li>
                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/Products/ViewData"> <i class="menu-icon fa fa-table"></i>Quản lý sản phẩm</a>

                    </li>
                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/News/ViewData"> <i class="menu-icon fa fa-th"></i>Quản lý tin tức</a>
                    </li>



                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/Users/ViewData"> <i class="menu-icon fa fa-tasks"></i>Quản lý người dùng</a>

                    </li>

                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/Suppliers/ViewData"> <i class="menu-icon fa fa-area-chart"></i>Quản lý nhà cung cấp</a>

                    </li>

                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/ImportBills/ViewData"> <i class="menu-icon fa fa-glass"></i>Quản lý hóa đơn nhập</a>
                    </li>
                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/ImportBillDetails/Index"> <i class="menu-icon fa fa-glass"></i>Chi tiết hóa đơn nhập</a>
                    </li>
                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/Orders/ViewData"> <i class="menu-icon fa fa-glass"></i>Quản lý hóa đơn bán</a>
                    </li>
                    <li class="menu-item-has-children dropdown">
                        <a href="/Admin/OrderDetails/ViewData"> <i class="menu-icon fa fa-glass"></i>Chi tiết hóa đơn bán</a>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside><!-- /#left-panel -->
    <!-- Left Panel -->
    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->
        <header id="header" class="header">

            <div class="header-menu">

                <div class="col-sm-7">
                    <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
                    <div class="header-left">
                        <button class="search-trigger"><i class="fa fa-search"></i></button>
                        <div class="form-inline">
                            <form class="search-form">
                                <input class="form-control mr-sm-2" type="text" placeholder="Search ..." aria-label="Search">
                                <button class="search-close" type="submit"><i class="fa fa-close"></i></button>
                            </form>
                        </div>

                        <div class="dropdown for-notification">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="notification" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-bell"></i>
                                <span class="count bg-danger">5</span>
                            </button>
                            <div class="dropdown-menu" aria-labelledby="notification">
                                <p class="red">You have 3 Notification</p>
                                <a class="dropdown-item media bg-flat-color-1" href="#">
                                    <i class="fa fa-check"></i>
                                    <p>Server #1 overloaded.</p>
                                </a>
                                <a class="dropdown-item media bg-flat-color-4" href="#">
                                    <i class="fa fa-info"></i>
                                    <p>Server #2 overloaded.</p>
                                </a>
                                <a class="dropdown-item media bg-flat-color-5" href="#">
                                    <i class="fa fa-warning"></i>
                                    <p>Server #3 overloaded.</p>
                                </a>
                            </div>
                        </div>

                        <div class="dropdown for-message">
                            <button class="btn btn-secondary dropdown-toggle" type="button"
                                    id="message"
                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="ti-email"></i>
                                <span class="count bg-primary">9</span>
                            </button>
                            <div class="dropdown-menu" aria-labelledby="message">
                                <p class="red">You have 4 Mails</p>
                                <a class="dropdown-item media bg-flat-color-1" href="#">
                                    <span class="photo media-left"><img alt="avatar" src="~/Areas/Admin/Data/images/avatar/1.jpg"></span>
                                    <span class="message media-body">
                                        <span class="name float-left">Jonathan Smith</span>
                                        <span class="time float-right">Just now</span>
                                    </span>
                                </a>
                                <a class="dropdown-item media bg-flat-color-4" href="#">
                                    <span class="photo media-left"><img alt="avatar" src="~/Areas/Admin/Data/images/avatar/2.jpg"></span>
                                    <span class="message media-body">
                                        <span class="name float-left">Jack Sanders</span>
                                        <span class="time float-right">5 minutes ago</span>
                                    </span>
                                </a>
                                <a class="dropdown-item media bg-flat-color-5" href="#">
                                    <span class="photo media-left"><img alt="avatar" src="~/Areas/Admin/Data/images/avatar/3.jpg"></span>
                                    <span class="message media-body">
                                        <span class="name float-left">Cheryl Wheeler</span>
                                        <span class="time float-right">10 minutes ago</span>
                                    </span>
                                </a>
                                <a class="dropdown-item media bg-flat-color-3" href="#">
                                    <span class="photo media-left"><img alt="avatar" src="~/Areas/Admin/Data/images/avatar/4.jpg"></span>
                                    <span class="message media-body">
                                        <span class="name float-left">Rachel Santos</span>
                                        <span class="time float-right">15 minutes ago</span>
                                    </span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="col-sm-5">
                    <div class="user-area dropdown float-right">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <img class="user-avatar rounded-circle" src="~/Areas/Admin/Data/images/admin.jpg" alt="User Avatar">
                        </a>

                        <div class="user-menu dropdown-menu">
                            <a class="nav-link" href="#"><i class="fa fa-user"></i> My Profile</a>

                            <a class="nav-link" href="#"><i class="fa fa-user"></i> Notifications <span class="count">13</span></a>

                            <a class="nav-link" href="#"><i class="fa fa-cog"></i> Settings</a>

                            <a class="nav-link" href="@Url.Action("Logout","Login")"><i class="fa fa-power-off"></i> Đăng xuất</a>
                        </div>
                        @*@{
                                var user = HttpContext.Current.Session["user"].ToString();
                               <span style="margin-right:10px;line-height:200%;">@user</span>
                            }*@
                        @{
                            <span style="margin-right:10px;line-height:200%;"> @Session["UserAdmin"] </span>
                        }
                    </div>

                    @*<div class="language-select dropdown" id="language-select">
                            <a class="dropdown-toggle" href="#" data-toggle="dropdown" id="language" aria-haspopup="true" aria-expanded="true">
                                <i class="flag-icon flag-icon-us"></i>
                            </a>
                            <div class="dropdown-menu" aria-labelledby="language">
                                <div class="dropdown-item">
                                    <span class="flag-icon flag-icon-fr"></span>
                                </div>
                                <div class="dropdown-item">
                                    <i class="flag-icon flag-icon-es"></i>
                                </div>
                                <div class="dropdown-item">
                                    <i class="flag-icon flag-icon-us"></i>
                                </div>
                                <div class="dropdown-item">
                                    <i class="flag-icon flag-icon-it"></i>
                                </div>
                            </div>
                        </div>*@

                </div>
            </div>

        </header><!-- /header -->
        <!-- Header-->
        <div class="content mt-3">
            <div class="animated fadeIn">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">

                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>


                </div>
            </div><!-- .animated -->
        </div><!-- .content -->


    </div><!-- /#right-panel -->


    <script src="~/Areas/Admin/Data/vendors/jquery/dist/jquery.min.js"></script>
    <script src="~/Areas/Admin/Data/vendors/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/Areas/Admin/Data/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Areas/Admin/Data/assets/js/main.js"></script>


    <script src="~/Areas/Admin/Data/vendors/chart.js/dist/Chart.bundle.min.js"></script>
    <script src="~/Areas/Admin/Data/assets/js/dashboard.js"></script>
    <script src="~/Areas/Admin/Data/assets/js/widgets.js"></script>
    <script src="~/Areas/Admin/Data/vendors/jqvmap/dist/jquery.vmap.min.js"></script>
    <script src="~/Areas/Admin/Data/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <script src="~/Areas/Admin/Data/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>

    <script src="~/Areas/Admin/Data/assets/Plugin/ckfinder/ckfinder.js"></script>
    <script src="~/Areas/Admin/Data/assets/Plugin/ckeditor/ckeditor.js"></script>

    <script src="~/Scripts/angular.min.js"></script>
    @RenderSection("Scripts", false)


</body>
</html>



